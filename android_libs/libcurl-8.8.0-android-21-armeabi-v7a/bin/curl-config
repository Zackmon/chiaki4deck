###########################################################################

prefix="/home/zackmon/.ndk-pkg/installed/android-21-armeabi-v7a/9dc01c0d4844d46dc41a4642fd310e1aa2ce53b7a86467ee09cfea8b274c6705"
# Used in ${exec_prefix}/lib
# shellcheck disable=SC2034
exec_prefix=${prefix}
# shellcheck disable=SC2034
includedir=${prefix}/include
cppflag_curl_staticlib=

usage()
{
  cat <<EOF
Usage: curl-config [OPTION]

Available values for OPTION include:

  --built-shared says 'yes' if libcurl was built shared
  --ca                  CA bundle install path
  --cc                  compiler
  --cflags              preprocessor and compiler flags
  --checkfor [version]  check for (lib)curl of the specified version
  --configure           the arguments given to configure when building curl
  --features            newline separated list of enabled features
  --help                display this help and exit
  --libs                library linking information
  --prefix              curl install prefix
  --protocols           newline separated list of enabled protocols
  --ssl-backends        output the SSL backends libcurl was built to support
  --static-libs         static libcurl library linking information
  --version             output version information
  --vernum              output version as a hexadecimal number
EOF

  exit "$1"
}

if test "$#" -eq 0; then
  usage 1
fi

while test "$#" -gt 0; do
  case "$1" in
  --built-shared)
    echo 'yes'
    ;;

  --ca)
    echo ''
    ;;

  --cc)
    echo '/home/zackmon/.ndk-pkg/core/wrapper-target-cc'
    ;;

  --prefix)
    echo "$prefix"
    ;;

  --feature|--features)
    for feature in AsynchDNS HSTS HTTP2 HTTPS-proxy IPv6 Largefile NTLM SSL TLS-SRP UnixSockets alt-svc brotli libz threadsafe zstd ""; do
      test -n "$feature" && echo "$feature"
    done
    ;;

  --protocols)
    # shellcheck disable=SC2043
    for protocol in DICT FILE FTP FTPS GOPHER GOPHERS HTTP HTTPS IMAP IMAPS IPFS IPNS MQTT POP3 POP3S RTSP SMB SMBS SMTP SMTPS TELNET TFTP; do
      echo "$protocol"
    done
    ;;

  --version)
    echo 'libcurl 8.8.0'
    exit 0
    ;;

  --checkfor)
    checkfor=$2
    cmajor=$(echo "$checkfor" | cut -d. -f1)
    cminor=$(echo "$checkfor" | cut -d. -f2)
    # when extracting the patch part we strip off everything after a
    # dash as that's used for things like version 1.2.3-pre1
    cpatch=$(echo "$checkfor" | cut -d. -f3 | cut -d- -f1)

    vmajor=$(echo '8.8.0' | cut -d. -f1)
    vminor=$(echo '8.8.0' | cut -d. -f2)
    # when extracting the patch part we strip off everything after a
    # dash as that's used for things like version 1.2.3-pre1
    vpatch=$(echo '8.8.0' | cut -d. -f3 | cut -d- -f1)

    if test "$vmajor" -gt "$cmajor"; then
      exit 0
    fi
    if test "$vmajor" -eq "$cmajor"; then
      if test "$vminor" -gt "$cminor"; then
        exit 0
      fi
      if test "$vminor" -eq "$cminor"; then
        if test "$cpatch" -le "$vpatch"; then
          exit 0
        fi
      fi
    fi

    echo "requested version $checkfor is newer than existing 8.8.0"
    exit 1
    ;;

  --vernum)
    echo '080800'
    exit 0
    ;;

  --help)
    usage 0
    ;;

  --cflags)
    if test "X$cppflag_curl_staticlib" = "X-DCURL_STATICLIB"; then
      CPPFLAG_CURL_STATICLIB="-DCURL_STATICLIB "
    else
      CPPFLAG_CURL_STATICLIB=""
    fi
    if test "X${prefix}/include" = "X/usr/include"; then
      echo "${CPPFLAG_CURL_STATICLIB}"
    else
      echo "${CPPFLAG_CURL_STATICLIB}-I${prefix}/include"
    fi
    ;;

  --libs)
    if test "X${exec_prefix}/lib" != "X/usr/lib" -a "X${exec_prefix}/lib" != "X/usr/lib64"; then
      CURLLIBDIR="-L${exec_prefix}/lib "
    else
      CURLLIBDIR=""
    fi
    if test "Xyes" = "Xno"; then
      echo "${CURLLIBDIR}-lcurl -lnghttp2 -lssl -lcrypto -lssl -lcrypto -ldl -pthread -lzstd -lbrotlidec -lbrotlicommon -lz"
    else
      echo "${CURLLIBDIR}-lcurl"
    fi
    ;;

  --ssl-backends)
    echo 'OpenSSL v3+'
    ;;

  --static-libs)
    if test "Xyes" != "Xno" ; then
      echo "${exec_prefix}/lib/libcurl.a" -L/home/zackmon/.ndk-pkg/run/1519689/android-21-armeabi-v7a/libcurl/lib -L/home/zackmon/.ndk-pkg/installed/android-21-armeabi-v7a/openssl-dev/lib -Wl,-rpath-link,/home/zackmon/.ndk-pkg/installed/android-21-armeabi-v7a/openssl-dev/lib -L/home/zackmon/.ndk-pkg/installed/android-21-armeabi-v7a/brotli/lib -Wl,-rpath-link,/home/zackmon/.ndk-pkg/installed/android-21-armeabi-v7a/brotli/lib -L/home/zackmon/.ndk-pkg/installed/android-21-armeabi-v7a/libz/lib -Wl,-rpath-link,/home/zackmon/.ndk-pkg/installed/android-21-armeabi-v7a/libz/lib -L/home/zackmon/.ndk-pkg/installed/android-21-armeabi-v7a/libzstd/lib -Wl,-rpath-link,/home/zackmon/.ndk-pkg/installed/android-21-armeabi-v7a/libzstd/lib -L/home/zackmon/.ndk-pkg/installed/android-21-armeabi-v7a/libcares/lib -Wl,-rpath-link,/home/zackmon/.ndk-pkg/installed/android-21-armeabi-v7a/libcares/lib -L/home/zackmon/.ndk-pkg/installed/android-21-armeabi-v7a/libnghttp2/lib -Wl,-rpath-link,/home/zackmon/.ndk-pkg/installed/android-21-armeabi-v7a/libnghttp2/lib -Wl,--as-needed -Wl,-z,muldefs -Wl,--allow-multiple-definition -flto -Wl,-s -L/home/zackmon/.ndk-pkg/installed/android-21-armeabi-v7a/libz/lib/pkgconfig/../../lib -L/home/zackmon/.ndk-pkg/installed/android-21-armeabi-v7a/openssl-dev/lib/pkgconfig/../../lib -L/home/zackmon/.ndk-pkg/installed/android-21-armeabi-v7a/libnghttp2/lib/pkgconfig/../../lib -lnghttp2 -lssl -lcrypto -lssl -lcrypto -ldl -pthread -lzstd -lbrotlidec -lbrotlicommon -lz
    else
      echo 'curl was built with static libraries disabled' >&2
      exit 1
    fi
    ;;

  --configure)
    echo " '--host=armv7a-linux-androideabi' '--prefix=/home/zackmon/.ndk-pkg/installed/android-21-armeabi-v7a/9dc01c0d4844d46dc41a4642fd310e1aa2ce53b7a86467ee09cfea8b274c6705' '--disable-option-checking' '--disable-nls' '--disable-rpath' '--enable-largefile' '--disable-debug' '--enable-static' '--enable-shared' '--disable-year2038' '--enable-optimize' '--enable-pthreads' '--with-default-ssl-backend=openssl' '--with-ssl=/home/zackmon/.ndk-pkg/installed/android-21-armeabi-v7a/openssl-dev' '--with-nghttp2' '--without-nghttp3' '--without-ngtcp2' '--without-quiche' '--without-libpsl' '--without-librtmp' '--without-libidn2' '--with-pic' 'CC=/home/zackmon/.ndk-pkg/core/wrapper-target-cc' 'CFLAGS=-Qunused-arguments -fPIC -Wall -fno-common -Os' 'CXX=/home/zackmon/.ndk-pkg/core/wrapper-target-c++' 'CXXFLAGS=-Qunused-arguments -fPIC -Wall -fno-common -Os' 'CPP=/home/zackmon/.ndk-pkg/core/wrapper-target-cc -E' 'CPPFLAGS=-I/home/zackmon/.ndk-pkg/run/1519689/android-21-armeabi-v7a/libcurl/include -I/home/zackmon/.ndk-pkg/installed/android-21-armeabi-v7a/openssl-dev/include -I/home/zackmon/.ndk-pkg/installed/android-21-armeabi-v7a/brotli/include -I/home/zackmon/.ndk-pkg/installed/android-21-armeabi-v7a/libz/include -I/home/zackmon/.ndk-pkg/installed/android-21-armeabi-v7a/libzstd/include -I/home/zackmon/.ndk-pkg/installed/android-21-armeabi-v7a/libcares/include -I/home/zackmon/.ndk-pkg/installed/android-21-armeabi-v7a/libnghttp2/include -Qunused-arguments -include /home/zackmon/.ndk-pkg/run/1519689/android-21-armeabi-v7a/libcurl/include/ndk-pkg-common.h -DNDEBUG' 'LDFLAGS=-L/home/zackmon/.ndk-pkg/run/1519689/android-21-armeabi-v7a/libcurl/lib -L/home/zackmon/.ndk-pkg/installed/android-21-armeabi-v7a/openssl-dev/lib -Wl,-rpath-link,/home/zackmon/.ndk-pkg/installed/android-21-armeabi-v7a/openssl-dev/lib -L/home/zackmon/.ndk-pkg/installed/android-21-armeabi-v7a/brotli/lib -Wl,-rpath-link,/home/zackmon/.ndk-pkg/installed/android-21-armeabi-v7a/brotli/lib -L/home/zackmon/.ndk-pkg/installed/android-21-armeabi-v7a/libz/lib -Wl,-rpath-link,/home/zackmon/.ndk-pkg/installed/android-21-armeabi-v7a/libz/lib -L/home/zackmon/.ndk-pkg/installed/android-21-armeabi-v7a/libzstd/lib -Wl,-rpath-link,/home/zackmon/.ndk-pkg/installed/android-21-armeabi-v7a/libzstd/lib -L/home/zackmon/.ndk-pkg/installed/android-21-armeabi-v7a/libcares/lib -Wl,-rpath-link,/home/zackmon/.ndk-pkg/installed/android-21-armeabi-v7a/libcares/lib -L/home/zackmon/.ndk-pkg/installed/android-21-armeabi-v7a/libnghttp2/lib -Wl,-rpath-link,/home/zackmon/.ndk-pkg/installed/android-21-armeabi-v7a/libnghttp2/lib -Wl,--as-needed -Wl,-z,muldefs -Wl,--allow-multiple-definition -flto -Wl,-s' 'AR=/home/zackmon/.ndk-pkg/uppm/installed/android-ndk-r26d/toolchains/llvm/prebuilt/linux-x86_64/bin/llvm-ar' 'RANLIB=/home/zackmon/.ndk-pkg/uppm/installed/android-ndk-r26d/toolchains/llvm/prebuilt/linux-x86_64/bin/llvm-ranlib' 'PKG_CONFIG=/home/zackmon/.ndk-pkg/uppm/installed/pkg-config/bin/pkg-config' 'PKG_CONFIG_PATH=/home/zackmon/.ndk-pkg/run/1519689/android-21-armeabi-v7a/libcurl/lib/pkgconfig:/home/zackmon/.ndk-pkg/installed/android-21-armeabi-v7a/libnghttp2/lib/pkgconfig:/home/zackmon/.ndk-pkg/installed/android-21-armeabi-v7a/libcares/lib/pkgconfig:/home/zackmon/.ndk-pkg/installed/android-21-armeabi-v7a/libzstd/lib/pkgconfig:/home/zackmon/.ndk-pkg/installed/android-21-armeabi-v7a/libz/lib/pkgconfig:/home/zackmon/.ndk-pkg/installed/android-21-armeabi-v7a/brotli/lib/pkgconfig:/home/zackmon/.ndk-pkg/installed/android-21-armeabi-v7a/openssl-dev/lib/pkgconfig' 'PKG_CONFIG_LIBDIR=/home/zackmon/.ndk-pkg/run/1519689/android-21-armeabi-v7a/libcurl/lib/pkgconfig' 'CC_FOR_BUILD=/home/zackmon/.ndk-pkg/core/wrapper-native-cc' 'host_alias=armv7a-linux-androideabi'"
    ;;

  *)
    echo "unknown option: $1"
    usage 1
    ;;
  esac
  shift
done

exit 0
